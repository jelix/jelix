{
    "name": "jelix/dependencies-resolver",
    "type": "library",
    "description": "Resolve dependencies between things to installe/remove/upgrade",
    "keywords": ["dependencies", "package"],
    "homepage": "http://jelix.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        }
    ],
    "require": {
        "php": ">=8.1",
        "jelix/version": "^1.0.0"
    },
    "require-dev" : {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4" : {
            "Jelix\\Dependencies\\" : "."
        }
    },
    "minimum-stability": "stable"
}
